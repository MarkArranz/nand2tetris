/**
 * Not gate:
 * out = not in
 */

 /* PROOF:
    NOT x
    <=> NOT (x AND x) [via Idempotency Law]
    <=> x NAND x [via Sheffer stroke]
 */

CHIP Not {
    IN in;
    OUT out;

    PARTS:
    Nand(a=in, b=in, out=out);
}